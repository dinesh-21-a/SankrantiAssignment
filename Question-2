//Question - 2
//Language - Python

class Solution:
    # @param A : tuple of integers
    # @return an integer
    def maxProfit(self, A):
        n = len(A)
        max_profit = 0
        dp = [0] * n
        for i in range(n-2, -1, -1):
            dp[i] = max(A[i+1], dp[i+1])
            
        for i in range(n-1):
            max_profit = max(max_profit, dp[i] - A[i])
                
        return max_profit
